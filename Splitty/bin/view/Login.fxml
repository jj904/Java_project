<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #7ca683;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="80.0" layoutY="30.0" prefHeight="269.0" prefWidth="241.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="224.0" text="Welcome Back!" textFill="#fcfafa">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial" size="31.0" />
               </font>
            </Label>
            <TextField fx:id="userID" promptText="Username">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="passID" promptText="Password">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="loginMessageLbl" alignment="CENTER" prefHeight="17.0" prefWidth="242.0" textAlignment="CENTER" textFill="#fcfafa">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="LoginLbl" mnemonicParsing="false" onAction="#loginButton" prefHeight="34.0" prefWidth="145.0" style="-fx-background-color: #eeeee7;" text="log in" textFill="#24554e">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
